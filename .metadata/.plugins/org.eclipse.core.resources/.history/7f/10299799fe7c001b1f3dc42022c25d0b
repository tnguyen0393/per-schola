package DogExercise;

public class Main {

	public static void main(String[] args) {
		MyDog lio = new MyDog("Dachshund", "Lio", 2, "Brown");
		MyDog mochi = new MyDog("Husky", "Mochi", 3, "White");
		
		lio.walk();
		lio.sleep();
		mochi.eat("hotdog");
		mochi.isAsleep();
		
		lio.wakeUp();
		lio.eat("eggs");
	}

}
