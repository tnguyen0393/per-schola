package DogExercise;

public class Main {

	public static void main(String[] args) {
		MyDog lio = new MyDog("Dachshund", "Lio", "Brown", 2019);
		MyDog mochi = new MyDog("Husky", "Mochi", "White", 2018);
		
		lio.walk();
		lio.sleep();
		mochi.eat("hotdog");
		mochi.isAsleep();
		
		lio.wakeUp();
		lio.eat("eggs");
		mochi.info();
		lio.info();
		
		System.out.println(MyDog.getHomeAddress());
	}

}
