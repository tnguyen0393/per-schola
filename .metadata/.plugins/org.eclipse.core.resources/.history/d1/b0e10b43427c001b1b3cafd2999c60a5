	public static void readFile(String loc) {
		File file = new File(loc);
		try {
			FileReader fr = new FileReader(file)
		} catch (FileNotFoundException e) {
			System.out.println("FILE NOT FOUND!!");
		} finally {
			System.out.println("This is finally block");
		}
	}
